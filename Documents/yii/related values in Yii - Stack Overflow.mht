 hide it
      survey.find("li").click(function() {
        var vote =3D survey.find("input:checked").val()
        if (vote) {
          castVoteForThisCreative(VoteEvent.downvote);
          castVoteForThisCreative(vote);
          showResponse();
        }
      });

    });
  });

})(this);

