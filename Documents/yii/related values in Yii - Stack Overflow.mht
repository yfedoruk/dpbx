(function(root) {

  var $ =3D root.jQuery;
  var BlockCookieName =3D 'azk-blocks';

  var VoteEvent =3D {
    upvote:        10,
    downvote:      11,
    uninteresting: 12,
    misleading:    13,
    offensive:     14,
    repetitive:    15
  };

  root.azRegisterExtension('adFeedback', function(divName, pixels) {
    if ($ =3D=3D=3D undefined) return;

    var ad =3D $('#' + divName);
    var voteInProgress =3D false;
    var voteComplete =3D false;

    var castVoteForThisCreative =3D function(type) {
      if (pixels[type] !=3D=3D undefined) {
        $('body').append("<img src=3D'" + pixels[type] + "' height=3D0 widt=
h=3D0 />");
      }
    };

    $(function() {
      ad.addClass('adzerk-vote').append(
        "<div class=3D'adzerk-vote-controls' style=3D'display:none;'>" +
          "<div class=3D'adzerk-vote-option adzerk-vote-up'><div class=3D'a=
dzerk-vote-icon'></div></div>" +
          "<div class=3D'adzerk-vote-option adzerk-vote-down'><div class=3D=
'adzerk-vote-icon'></div></div>" +
        "</div>" +
        "<div class=3D'adzerk-vote-survey' style=3D'display:none;'>" +
          "<form>" +
            "<span>No problem. We won't show you that ad again. Why didn't=
 you like it?</span>" +
            "<ul>" +
              "<li><label><input type=3D'radio' value=3D'" + VoteEvent.unin=
teresting + "' name=3D'downvoteReason' />Uninteresting</label></li>" +
              "<li><label><input type=3D'radio' value=3D'" + VoteEvent.misl=
eading    + "' name=3D'downvoteReason' />Misleading</label></li>" +
              "<li><label><input type=3D'radio' value=3D'" + VoteEvent.offe=
nsive     + "' name=3D'downvoteReason' />Offensive</label></li>" +
              "<li><label><input type=3D'radio' value=3D'" + VoteEvent.repe=
titive    + "' name=3D'downvoteReason' />Repetitive</label></li>" +
            "</ul>" +
            "<a href=3D'#' class=3D'adzerk-vote-cancel'>Oops! I didn't mean=
 to do this.</a>" +
          "</form>" +
        "</div>");

      var controls =3D ad.find(".adzerk-vote-controls");
      var survey   =3D ad.find(".adzerk-vote-survey");

      var showResponse =3D function() {
        voteInProgress =3D false;
        voteComplete =3D true;
        survey.hide();
        var thanks =3D $("<div class=3D'adzerk-vote-response'>Thanks!</div>=
");
        thanks.appendTo(controls).hide().fadeIn(250).delay(3000).fadeOut(25=
0, function() {
          $(this).remove();
        });
      };

      // Show vote controls when hovering over ad
      ad.hover(
        function() {
          if (!voteInProgress) {
            controls.stop(true, true).fadeIn(250);
          }
        },
        function() {
          if (!voteInProgress) {
            controls.stop(true, true).fadeOut(250);
          }
        }
      );

      // When up arrow is clicked, display a thank you message
      controls.find(".adzerk-vote-up").click(function() {
        if (!voteInProgress && !voteComplete) {
          $(this).addClass("selected");
          castVoteForThisCreative(VoteEvent.upvote);
          showResponse();
        }
        return false;
      });

      // When down arrow is clicked, display the survey to ask why the user=
 didn't like the ad
      controls.find(".adzerk-vote-down").click(function() {
        if (!voteInProgress && !voteComplete) {
          $(this).addClass("selected");
          voteInProgress =3D true;
          survey.fadeIn(250);
        }
        return false;
      });

