<script>
//Функция jQuery outerHTML

//Очень полезная, а самое главное кроссбраузерная функция outerHTML на  jQuery.
    jQuery.fn.outerHTML = function() {
        return jQuery("<p>").append(this.clone()).html();
    }
    
//Реализация очень оригинальная :)

$(this).clone().wrap('<p>').parent().html();

//или для любого =>

objDOM.clone().wrap('<p>').parent().html();

</script>
